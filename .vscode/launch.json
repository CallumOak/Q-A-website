    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET CORE Launch (console)",
            "type": "coreclr",
            "request": "launch",
            //"preLaunchTask": "build",
            "program": "${workspaceFolder}/prid/prid/bin/Debug/netcoreapp2.2/prid.dll",
            "args":[],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".Net Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}